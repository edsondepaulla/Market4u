#RewriteEngine On
#RewriteCond %{HTTP_REFERER} !^https://(.+\.)?market4u\.com.br/ [NC]
#RewriteRule .*\.*$ - [F]
Header set Access-Control-Allow-Origin "http://127.0.0.1:8080"
